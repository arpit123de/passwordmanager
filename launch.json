{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Flask App",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/venv/Scripts/flask.exe",
      "args": [
        "run"
      ],
      "env": {
        "FLASK_APP": "app.py",
        "FLASK_ENV": "development"
      },
      "jinja": true
    }
  ]
}
